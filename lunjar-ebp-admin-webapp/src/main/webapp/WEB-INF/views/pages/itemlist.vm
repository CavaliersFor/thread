	/**
	*订单状态
	*/
	function tradeStatus(status,type){
		if(type==1){
			//查询订单状态
			jQuery("#status").val(status);
			submitForm1();
		}else if(type==2){
			//查询近三个月和前三个月
			jQuery("#time").val(status);
			jQuery("#status").val("");
			submitForm1();
		}
	}
	/***
	* 加减月份
	**/
	function addDate(date,month){ 
		var d=new Date(date); 
		//d.setDate(d.getDate()+days); 
		d.setMonth(d.getMonth()+month);
		var month=d.getMonth()+1;
		var day = d.getDate(); 
		if(month<10){ 
		month = "0"+month; 
		} 
		if(day<10){ 
		day = "0"+day; 
		} 
		var val = d.getFullYear()+"-"+month+"-"+day; 
		return val; 
	}
	
	/**
	*添加备注
	*str1 是原备注的值
	*str2 是订单号
	*type是不同的类型标识
	***/
	function addRemark(str1,str2,type){
		var tradeId = str2;
		if(type==3){
			//是批量增加备注
			var val = jQuery("input[name='tradeIds']:checked");
			if(val.length == 0){
				jQuery.alert({
					type: 'error',
					text: '您还未选择任何订单',
					time: 3000
				});
				return ;
			}else{
				val.each(function(index){
					if(index==(val.length-1)){
						tradeId = tradeId+jQuery(this).val();
					}else{
						tradeId = tradeId+jQuery(this).val()+",";
					}
				});
			}
		}
		
		jQuery("#remarkTradeIds").val(tradeId);
		jQuery("#remark").val(str1);
		//显示弹层和遮罩
		$('#myModal').fadeIn(100);
		addMark();
	}
