	 function searchLogisName(self){
		 var name = self.val();
		 jQuery.ajax({
				url:"/trade/queryLogistics",
				data:"name="+name,
				type:"post",
				dataType:"json",
				success:function(data){
					if(data!=null && data.success){
						var html = '';
						if(data.list!=null){
							for(var i=0;i<data.list.length;i++){
								html = html+'<dd class="">'+data.list[i].name+'</dd>';
